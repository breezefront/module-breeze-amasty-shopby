<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="better_compatibility" xsi:type="array">
                    <item name="Amasty_Shopby" xsi:type="boolean">true</item>
                </argument>
                <argument name="ignore" xsi:type="array">
                    <item name="tooltip" xsi:type="string">mage/tooltip</item>
                    <item name="touch-punch" xsi:type="string">Amasty_Shopby/js/jquery.ui.touch-punch.min</item>
                    <item name="chosen" xsi:type="string">Amasty_ShopbyBase/js/chosen/chosen.jquery</item>
                </argument>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="amShopbyFilterFromTo" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterFromTo</item>
                            </item>
                            <item name="amShopbySwiperSlider" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbySwiperSlider</item>
                            </item>
                            <item name="amShopbyFilterSlider" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterSlider</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
