<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="amShopbyHelpers" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/utils/helpers</item>
                            </item>

                            <item name="Amasty_Shopby/js/amShopby" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/amShopby</item>
                            </item>

                            <item name="amShopby-register" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopby-register</item>
                            </item>

                            <item name="Amasty_Shopby/js/components/amShopbyFilterContainer" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/components/amShopbyFilterContainer</item>
                            </item>

                            <item name="amShopbyFilterContainer-register" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyFilterContainer-register</item>
                            </item>

                            <item name="Amasty_Shopby/js/components/amShopbyFilterFromTo" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterFromTo</item>
                            </item>

                            <item name="amShopbyFilterFromTo-register" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyFilterFromTo-register</item>
                            </item>

                            <item name="Amasty_Shopby/js/amShopbyFiltersSync" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyFiltersSync</item>
                            </item>

                            <item name="Amasty_Shopby/js/amShopbyTopFilters" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyTopFilters</item>
                            </item>

                            <item name="Amasty_Shopby/js/amShopbyResponsive" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyResponsive</item>
                            </item>

                            <item name="Amasty_Shopby/js/components/amShopbyFilterCollapse" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/components/amShopbyFilterCollapse</item>
                            </item>

                            <item name="amShopbyFilterCollapse-register" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyFilterCollapse-register</item>
                            </item>

                            <item name="amShopbySwiperSlider" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbySwiperSlider</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
